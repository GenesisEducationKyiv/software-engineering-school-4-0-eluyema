generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Subscription {
  id    String @id @default(uuid())
  email String @unique
}

model Rate {
  id   String   @id @default(uuid())
  name String   @unique
  rate Float
  date DateTime
}
